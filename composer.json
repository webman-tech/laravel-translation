{
  "name": "webman-tech/laravel-translation",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/laravel-translation",
  "require": {
    "php": ">=7.2",
    "illuminate/translation": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8|^9",
    "symfony/translation": ">=5.0",
    "workerman/webman-framework": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "WebmanTech\\LaravelTranslation\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WebmanTech\\LaravelTranslation\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --testdox --no-interaction"
  }
}
