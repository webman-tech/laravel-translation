{
  "name": "webman-tech/laravel-translation",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/laravel-translation",
  "require": {
    "php": "^8.2",
    "illuminate/translation": "^11.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.7",
    "workerman/webman-framework": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "WebmanTech\\LaravelTranslation\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WebmanTech\\LaravelTranslation\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "pest"
  }
}
